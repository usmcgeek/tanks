################################################################## Automatic Text Replacements
auto replace:
	match: (C)
	replace: ©
auto replace:
	match: (c)
	replace: ©
auto replace:
	match: CR
	replace: ©
auto replace:
	match: TM
	replace: ™
	whole word: true
auto replace:
	match: --
	replace: —
auto replace:
	match: ---
	replace: —
auto replace:
	match: ::
	replace: •

